package com.example.demo;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.annotation.ComponentScan;

import com.lewis.web.BookController;
import com.lewis.web.HelloController;

@SpringBootApplication
@ComponentScan("com.lewis.web.HelloController")
@ComponentScan("com.lewis.web.BookController")
public class Demo3Application {

	public static void main(String[] args) {
		//SpringApplication.run(HelloController.class, args);
		SpringApplication.run(BookController.class,args);
	}
}
